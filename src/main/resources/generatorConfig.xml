        <!--文章-->
      <table tableName="cms_article" domainObjectName="Article" >
          <columnOverride column="context" javaType="java.lang.String" jdbcType="VARCHAR" />
          <columnOverride column="source" javaType="java.lang.String" jdbcType="VARCHAR" />
      </table>
